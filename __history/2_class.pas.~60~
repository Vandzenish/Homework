program hm;

{$APPTYPE CONSOLE}

uses
    SysUtils;
    
var
    mas : array [0..100] of integer;
    I, a, ram, count : integer;
label
    Lab;
begin

    for I := 0 to high(mas) do  begin

        Lab:count:=0;
        ram:=random(high(mas));
        for a := 0 to I do begin

            if ram = mas[a] then
                break
            else begin
                count:=count+1;
            end;
         if count = I then mas[i]:=ram;
         write(mas[i], ' ', ram);
         if mas[i]=0 then goto Lab;
         end;
        write(mas[i], '-- ');
    end;





end.
