program hw;
{$APPTYPE CONSOLE}


uses
    SysUtils;

var
    I : byte;
    sum : longword;


begin
   sum:=1;
   for I := 1 to 10 do begin
        sum := sum * I;
        if I=10 then writeln('Факториал 10 - ', sum);
   end;
   for I := 1 to 15 do begin
        sum := sum * I;
        if I=15 then writeln('Факториал 15 - ', sum);
   end;
   for I := 1 to 25 do begin
        sum := sum * I;
        if I=25 then writeln('Факториал 25 - ', sum);
   end;





end.
