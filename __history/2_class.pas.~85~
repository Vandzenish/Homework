program hm;

{$APPTYPE CONSOLE}

uses
    SysUtils;
    
var
    mas : array of integer;
    I, a, b, ram, N : integer;
label
    Lab;
begin
    write('Enter N :');
    Readln(N);

    setlength(mas, N);
    for I := 0 to high(mas) do  begin
        Lab:
        ram:=random(high(mas)+2); // Почему-то нужно +2 иначе идёт
           // write(' -',ram, '- ');    //    в бесконечный перебор
            for a := 0 to I do if ram=mas[a] then goto Lab;
        mas[i]:=ram;

        //write(mas[i], ' ');
    end;
      for b := 0 to high(mas) do begin
            if mas[b]>(high(mas)/2+1) then write(mas[b], ' ')
            
        end;




end.
