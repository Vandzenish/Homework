program hm;

{$APPTYPE CONSOLE}

uses
    SysUtils,
    Math;
var
    mas : array of integer;
    I, a, b, ram, N, sum1 : integer;
label
    Lab;
begin
    sum1:=0;
    write('Enter N :');
    Readln(N);

    setlength(mas, N);
    for I := 0 to high(mas) do  begin  // цикл дл€ генерации слйчайного N-ого массива
        Lab:
        ram:=random(high(mas)+2); // ѕочему-то нужно +2 иначе идЄт
           // write(' -',ram, '- ');    //    в бесконечный перебор
            for a := 0 to I do if ram=mas[a] then goto Lab;
        mas[i]:=ram;

        //write(mas[i], ' ');    // дл€ проверки значений массива
    end;
      for b := 0 to high(mas) div 2 do begin   // цикл дл€ действий с массивом
            //if mas[b]>(high(mas)/2+1) then write(mas[b], ' ') // от задани€ 2.4
            sum1:= sum1 + mas[b];
      end;
        write('Sum: ', sum1);



end.
